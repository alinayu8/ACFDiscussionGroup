
<h3>create a cell group</h3><br>

<%= simple_form_for(CellGroup.new) do |f| %>
    <%= f.error_notification %>
    <div class="row">
        <div class="form-inputs">
            <%= f.input :name %>
            <%= f.collection_radio_buttons :gender, [["Male", "Male"] ,["Female", "Female"]], :first, :last, label: true  %>
        </div>
    </div>
    <div class="row">
        <div class="form-actions col s8">
            <%= f.button :submit %>
        </div>
</div>
<% end %>

